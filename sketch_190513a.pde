void setup() {
  size(600, 300); 
  stroke(255);
  noFill();
}

void draw() {
  background(0);
  for (int i = 0; i < 200; i += 20)
  {
    bezier(600+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(500+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(400+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(300+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(200+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(100+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
    bezier(10+(1.0), 30+i, 510, 20, 440, 300, 240-(70.0), 300+(200.0));
  }
  
  
}
